{
  "editor.fontFamily": "Cascadia Code",
  "cSpell.language": "en, es",
  "workbench.colorCustomizations": {
    "editor.selectionBackground": "#537188",
    "sideBar.background": "#1e2022",
    "sideBarSectionHeader.background": "#52616b",
    "sideBarTitle.foreground": "#90be6d",
    "minimap.background": "#2E3840",
    "titleBar.activeBackground": "#2E3840",
    "titleBar.activeForeground": "#f0f5f9",
    "terminal.background": "#1e2022",
    "terminal.foreground": "#F0EDD4",
    "activityBar.activeBackground": "#393e41",
    "activityBar.background": "#0a0a0a",
    "statusBar.background": "#4f4557",
    "statusBar.foreground": "#F0EDD4",
    "panelTitle.activeForeground": "#F0EDD4",
    "panelTitle.inactiveForeground": "#90be6d",
    "list.errorForeground": "#b3e5ec",
    "list.warningForeground": "#00d9ff",
    "gitDecoration.modifiedResourceForeground": "#00ffb3",
    "gitDecoration.untrackedResourceForeground": "#619b8a",
    "panel.background": "#90be6d1A",
    "tab.activeForeground": "#2fbf71",
    "tab.inactiveForeground": "#e76f51",
    "tab.activeBackground": "#1e2022",
    "tab.inactiveBackground": "#1e2022",
    "tab.border": "#1e2022",
    "editorGroupHeader.tabsBackground": "#1e2022",
    "editorGroupHeader.tabsBorder": "#1e2022",
    "editorGroup.border": "#1e2022",
    "editorGroup.background": "#1e2022",
    "editorGroupHeader.noTabsBackground": "#1e2022",
    "editorGroupHeader.noTabsBorder": "#1e2022",
    "editorGroup.emptyBackground": "#1e2022"
  },
  "background.enabled": true,
  "background.fullscreen": {
    "image": "https://images.alphacoders.com/985/thumb-1920-985802.png", // url of your image
    // "image": ["https://pathtoimage.png"], // An array may be useful when set interval for carousel
    "opacity": 0.95, // 0.85 ~ 0.95 recommended
    "size": "cover", // also css, `cover` to self-adaption (recommended)，or `contain`、`200px 200px`
    "position": "center", // alias to `background-position`, default `center`
    "interval": 0 // seconds of interval for carousel, default `0` to disabled.
  },
  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "name": "blah",
        "scope": "text.html.derivative",
        "settings": {
          "foreground": "#F0EDD4",
          "fontStyle": "bold"
        }
      },
      {
        "name": "htmlTag",
        "scope": "entity.name.tag",
        "settings": {
          "foreground": "#E06469"
        }
      }
    ]
  },

  "editor.minimap.enabled": false,
  "editor.hideCursorInOverviewRuler": true,
  "editor.overviewRulerBorder": false,
  "editor.scrollbar.horizontal": "hidden",
  "editor.scrollbar.vertical": "hidden",
  "editor.minimap.showSlider": "always",
  "breadcrumbs.enabled": false,
  "editor.minimap.size": "fill",
  "workbench.tree.renderIndentGuides": "none",
  "explorer.openEditors.visible": 0,
  "editor.minimap.renderCharacters": false,
  "explorer.decorations.colors": false,
  "editor.guides.highlightActiveBracketPair": false,
  "editor.guides.bracketPairsHorizontal": false,
  "editor.bracketPairColorization.enabled": false,
  "editor.guides.indentation": false,
  "editor.cursorBlinking": "phase",
  "editor.cursorSmoothCaretAnimation": "on",
  "editor.renderLineHighlight": "none",
  "editor.glyphMargin": false,
  "editor.lineNumbers": "off",
  "editor.showFoldingControls": "never",
  "workbench.layoutControl.type": "menu",
  "window.title": " ",
  "editor.renderControlCharacters": false,
  "editor.renderWhitespace": "none",
  "workbench.statusBar.visible": false,
  "workbench.activityBar.visible": false,
  "workbench.panel.opensMaximized": "never",
  "workbench.layoutControl.enabled": true,
  "workbench.sideBar.location": "left"
}
